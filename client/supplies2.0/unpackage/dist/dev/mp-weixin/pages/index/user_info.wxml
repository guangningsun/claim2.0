<view><cu-custom vue-id="1a4102ca-1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">信息登记</view></cu-custom><view class="cu-card"><view class="cu-item"><form><view class="cu-form-group margin-top"><text class="cuIcon-title text-gray"></text><view class="title">昵称</view><input class="text-right" placeholder="请输入昵称" name="input" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></view><view class="cu-form-group"><text class="cuIcon-title text-red"></text><text class="title">手机号码</text><input class="text-right" placeholder="请输入手机号码" name="input" disabled="{{true}}" data-event-opts="{{[['input',[['__set_model',['','tel_num','$event',[]]],['checkBtnEnable',['$event']]]]]}}" value="{{tel_num}}" bindinput="__e"/></view><view class="cu-form-group"><text class="cuIcon-title text-red"></text><text class="title">姓名</text><input class="text-right" placeholder="请填写真实姓名,以便管理员审核" name="input" data-event-opts="{{[['input',[['__set_model',['','user_name','$event',[]]],['checkBtnEnable',['$event']]]]]}}" value="{{user_name}}" bindinput="__e"/></view><view class="cu-form-group"><text class="cuIcon-title text-red"></text><text class="title">部门</text><picker value="{{apartment_picker_index}}" range="{{apartment_picker}}" data-event-opts="{{[['change',[['apartPickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(apartment_picker_index>-1?apartment_picker[apartment_picker_index]:'请选择所属部门')+''}}</view></picker></view><view class="cu-form-group"><text class="cuIcon-title text-gray"></text><view class="title">物品送达常用地址</view><input class="text-right" placeholder="选填,请填写工作地点具体门牌号" name="input" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view></form></view><view class="justify-between bottom-box"><view class="padding flex flex-direction"><button class="cu-btn bg-blue lg" dis